<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.circularreveal.cardview.CircularRevealCardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="@dimen/dp_12">

        <TextView
            android:id="@+id/text_view_booking_data_id_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/booking_id_label"
            android:textAppearance="?attr/textAppearanceBody2"
            android:textColor="?attr/colorTextMedium"
            app:layout_constraintTop_toTopOf="@id/text_view_booking_data_id"
            app:layout_constraintBottom_toBottomOf="@id/text_view_booking_data_id"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/text_view_booking_data_id"
            android:layout_width="@dimen/dp_0"
            android:layout_height="wrap_content"
            android:textAlignment="textEnd"
            android:textAppearance="?attr/textAppearanceBody1"
            android:textColor="?attr/colorOnSurface"
            android:textStyle="bold"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/text_view_booking_data_id_label"
            tools:text="BOOKING_ID" />

        <TextView
            android:id="@+id/text_view_booking_data_date_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/booking_date_label"
            android:textAppearance="?attr/textAppearanceBody2"
            android:textColor="?attr/colorTextMedium"
            app:layout_constraintTop_toTopOf="@id/text_view_booking_data_date"
            app:layout_constraintBottom_toBottomOf="@id/text_view_booking_data_date"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/text_view_booking_data_date"
            android:layout_width="@dimen/dp_0"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_12"
            android:textAlignment="textEnd"
            android:textAppearance="?attr/textAppearanceBody1"
            android:textColor="?attr/colorOnSurface"
            app:layout_constraintTop_toBottomOf="@id/text_view_booking_data_id"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/text_view_booking_data_date_label"
            tools:text="Wed, 12 Apr 2021, 19:38" />

        <TextView
            android:id="@+id/text_view_booking_data_status_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/booking_status_label"
            android:textAppearance="?attr/textAppearanceBody2"
            android:textColor="?attr/colorTextMedium"
            app:layout_constraintTop_toTopOf="@id/text_view_booking_data_status"
            app:layout_constraintBottom_toBottomOf="@id/text_view_booking_data_status"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/text_view_booking_data_status"
            android:layout_width="@dimen/dp_0"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_12"
            android:textAlignment="textEnd"
            android:textAppearance="?attr/textAppearanceBody1"
            android:textColor="?attr/colorOnSurface"
            app:layout_constraintTop_toBottomOf="@id/text_view_booking_data_date"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/text_view_booking_data_status_label"
            tools:text="On going" />

        <View
            android:id="@+id/view_booking_data_separator"
            style="@style/View.QHope.Separator"
            android:layout_width="match_parent"
            android:layout_marginTop="@dimen/dp_16"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/text_view_booking_data_status"
            app:layout_constraintBottom_toTopOf="@id/text_view_booking_data_hospital_label" />

        <TextView
            android:id="@+id/text_view_booking_data_hospital_label"
            android:layout_width="@dimen/dp_0"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_12"
            android:text="@string/booked_hospital_label"
            android:textAppearance="?attr/textAppearanceHeadline4"
            android:textColor="?attr/colorOnSurface"
            app:layout_constraintTop_toBottomOf="@id/view_booking_data_separator"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/image_view_booking_data_hospital"
            android:layout_width="@dimen/dp_64"
            android:layout_height="@dimen/dp_64"
            android:layout_marginTop="@dimen/dp_12"
            android:clickable="true"
            android:focusable="true"
            app:shapeAppearance="?attr/shapeAppearanceMediumComponent"
            app:layout_constraintTop_toBottomOf="@id/text_view_booking_data_hospital_label"
            app:layout_constraintBottom_toTopOf="@id/view_booking_data_separator_2"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/text_view_booking_data_hospital_name"
            android:layout_width="@dimen/dp_0"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_8"
            android:clickable="true"
            android:ellipsize="end"
            android:focusable="true"
            android:singleLine="true"
            android:textAppearance="?attr/textAppearanceHeadline5"
            android:textColor="?attr/colorOnSurface"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/image_view_booking_data_hospital"
            tools:text="Hospital"
            app:layout_constraintTop_toTopOf="@id/image_view_booking_data_hospital"
            app:layout_constraintBottom_toTopOf="@+id/text_view_booking_data_hospital_type" />

        <TextView
            android:id="@+id/text_view_booking_data_hospital_type"
            android:layout_width="@dimen/dp_0"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_8"
            android:layout_marginTop="@dimen/dp_4"
            android:textAppearance="?attr/textAppearanceBody2"
            android:textColor="?attr/colorTextMedium"
            app:layout_constraintTop_toBottomOf="@+id/text_view_booking_data_hospital_name"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/image_view_booking_data_hospital"
            app:layout_constraintBottom_toTopOf="@+id/text_view_booking_data_hospital_address"
            tools:text="Rumah sakit umum" />

        <TextView
            android:id="@+id/text_view_booking_data_hospital_address"
            android:layout_width="@dimen/dp_0"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_8"
            android:layout_marginTop="@dimen/dp_8"
            android:ellipsize="end"
            android:singleLine="true"
            android:textAppearance="?attr/textAppearanceBody1"
            android:textColor="?attr/colorTextMedium"
            app:layout_constraintTop_toBottomOf="@+id/text_view_booking_data_hospital_type"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/image_view_booking_data_hospital"
            app:layout_constraintBottom_toBottomOf="@id/image_view_booking_data_hospital"
            tools:text="Alamat hospital" />

        <View
            android:id="@+id/view_booking_data_separator_2"
            style="@style/View.QHope.Separator"
            android:layout_width="match_parent"
            android:layout_marginTop="@dimen/dp_16"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/image_view_booking_data_hospital"
            app:layout_constraintBottom_toTopOf="@id/text_view_booking_data_hospital_info_label" />

        <TextView
            android:id="@+id/text_view_booking_data_hospital_info_label"
            android:layout_width="@dimen/dp_0"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_12"
            android:text="@string/booking_info_label"
            android:textAppearance="?attr/textAppearanceHeadline4"
            android:textColor="?attr/colorOnSurface"
            app:layout_constraintTop_toBottomOf="@id/view_booking_data_separator_2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/text_view_booking_data_check_in_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/check_in_at_label"
            android:textAppearance="?attr/textAppearanceBody2"
            android:textColor="?attr/colorTextMedium"
            app:layout_constraintTop_toTopOf="@id/text_view_booking_data_check_in"
            app:layout_constraintBottom_toBottomOf="@id/text_view_booking_data_check_in"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/text_view_booking_data_check_in"
            android:layout_width="@dimen/dp_0"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_12"
            android:textAlignment="textEnd"
            android:textAppearance="?attr/textAppearanceBody1"
            android:textColor="?attr/colorOnSurface"
            app:layout_constraintTop_toBottomOf="@id/text_view_booking_data_hospital_info_label"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/text_view_booking_data_check_in_label"
            tools:text="Wed, 12 Apr 2021, 19:38" />

        <TextView
            android:id="@+id/text_view_booking_data_check_out_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/check_out_at_label"
            android:textAppearance="?attr/textAppearanceBody2"
            android:textColor="?attr/colorTextMedium"
            app:layout_constraintTop_toTopOf="@id/text_view_booking_data_check_out"
            app:layout_constraintBottom_toBottomOf="@id/text_view_booking_data_check_out"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/text_view_booking_data_check_out"
            android:layout_width="@dimen/dp_0"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_12"
            android:textAlignment="textEnd"
            android:textAppearance="?attr/textAppearanceBody1"
            android:textColor="?attr/colorOnSurface"
            app:layout_constraintTop_toBottomOf="@id/text_view_booking_data_check_in"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/text_view_booking_data_check_out_label"
            tools:text="Wed, 12 Apr 2021, 19:38" />

        <androidx.constraintlayout.widget.Group
            android:id="@+id/group_booking_data_check_out"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone"
            app:constraint_referenced_ids="text_view_booking_data_check_out, text_view_booking_data_check_out_label"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/text_view_booking_data_room_cost_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/room_cost_per_day_label"
            android:textAppearance="?attr/textAppearanceBody2"
            android:textColor="?attr/colorTextMedium"
            app:layout_constraintTop_toTopOf="@id/text_view_booking_data_room_cost"
            app:layout_constraintBottom_toBottomOf="@id/text_view_booking_data_room_cost"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/text_view_booking_data_room_cost"
            android:layout_width="@dimen/dp_0"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_12"
            android:textAlignment="textEnd"
            android:textAppearance="?attr/textAppearanceBody1"
            android:textColor="?attr/colorSecondaryVariant"
            android:textStyle="bold"
            app:layout_constraintTop_toBottomOf="@id/text_view_booking_data_check_out"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/text_view_booking_data_room_cost_label"
            tools:text="Rp380.000,00" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</com.google.android.material.circularreveal.cardview.CircularRevealCardView>